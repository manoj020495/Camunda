zeebe:
  broker:
    exporters:
      rdbms:
        className: io.camunda.zeebe.exporter.rdbms.RdbmsExporter
        jarPath: lib/rdbms-exporter-8.x.x.jar   # CHANGE TO YOUR ACTUAL JAR FILENAME
        args:
          url: "jdbc:oracle:thin:@//your-oracle-host:1521/your-service"
          username: "your-oracle-username"
          password: "your-oracle-password"
          driver: "oracle.jdbc.OracleDriver"
          tables:
            process_instances: "process_instance"
            user_tasks: "user_task"
