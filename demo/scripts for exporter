CREATE TABLE user_task (
  userTaskKey NUMBER(19) PRIMARY KEY,
  elementId VARCHAR2(255),
  name VARCHAR2(255),
  processDefinitionId VARCHAR2(255),
  creationDate TIMESTAMP WITH TIME ZONE,
  completionDate TIMESTAMP WITH TIME ZONE,
  assignee VARCHAR2(255),
  state VARCHAR2(50),
  formKey NUMBER(19),
  processDefinitionKey NUMBER(19),
  processInstanceKey NUMBER(19),
  elementInstanceKey NUMBER(19),
  tenantId VARCHAR2(255),
  dueDate TIMESTAMP WITH TIME ZONE,
  followUpDate TIMESTAMP WITH TIME ZONE,
  candidateGroups CLOB,
  candidateUsers CLOB,
  externalFormReference VARCHAR2(4000),
  processDefinitionVersion NUMBER(10),
  customHeaders CLOB,
  priority NUMBER(10),
  partitionId NUMBER(10)
);
